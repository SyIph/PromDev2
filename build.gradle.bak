buildscript {
  repositories {
    jcenter()
  }
  dependencies {
    classpath 'edu.sc.seis.launch4j:launch4j:2.5.1'
  }
}

repositories {
  jcenter()
}

apply plugin: 'java'
apply plugin: 'edu.sc.seis.launch4j'

launch4j {
  mainClassName = 'SwingDemo'
}

dependencies {
    compile 'org.slf4j:slf4j-api:1.7.7'

    testCompile 'junit:junit:4.11'
}
